#alias grp="git rebase --hard $(git rev-parse --abbrev-ref HEAD | sed -r 's/_(incorrect|secure)(_[0-2])?//g')"
alias g='git'
alias ga='git add'
alias gaa='git add -A'
alias gap='git add -p'
alias gb='git branch'
alias gca='git commit --amend --no-edit'
alias gcd='git checkout develop'
alias gcm='git checkout master'
alias gcmsg='git commit -m'
alias gco='git checkout'
alias gcob='git checkout -b'
alias gcp='git cherry-pick'
alias gcpc='git cherry-pick --continue'
alias gcpn='git cherry-pick --no-commit'
alias gd='git diff'
alias gdc='git diff --cached'
alias gdd='git diff develop'
alias gdl='git diff HEAD^ HEAD'
alias gdm='git diff master'
alias gdr='git diff origin/$(git rev-parse --abbrev-ref HEAD)'
alias gf='git fetch'
alias gl='git log --pretty=format:"%ad | %C(yellow)%h%C(reset) | %an | %C(blue)%s%C(reset) (%ar)" --date="short"'
alias gld='gl develop..$(git rev-parse --abbrev-ref HEAD)'
alias glm='gl master..$(git rev-parse --abbrev-ref HEAD)'
alias gmd='git merge develop'
alias gpl='git pull'
alias gplr='git pull --rebase'
alias gps='git push'
alias gpsf='git push --force'
alias gpsu='git push -u origin $(git rev-parse --abbrev-ref HEAD)'
alias gr='git reset'
alias grc='git rebase --continue'
alias grd='git rebase develop'
alias grh='git reset HEAD~'
alias grm='git rebase master'
alias grs='git rebase --skip'
alias gs='git status'
alias gst='git stash --include-untracked'
alias gstp='git stash pop'
alias gwip='git commit -m "WIP"'
alias this_branch='git rev-parse --abbrev-ref HEAD'

