alias ip='ifconfig |grep "inet "'
alias vim='/usr/local/opt/macvim/MacVim.app/Contents/MacOS/Vim'
alias zshconf='$EDITOR ~/.zshrc'
alias vconf='$EDITOR ~/.vimrc'
alias tconf='$EDITOR ~/.tmux.conf'
alias fishconf='$EDITOR ~/.config/fish/config.fish'
alias st='subl'
alias at='atom'
alias hosts='$EDITOR /private/etc/hosts'
alias reload='. ~/.zshrc'
alias lsa='ls -a'
alias lla='ll -a'
alias cls='clear'
alias ..='cd ..'
alias ...='cd ../..'
alias dotfiles='cd ~/dev/dotfiles'
alias vimstall='vim +PluginInstall +qall'
